{
  "name": "atom-elixir-formatter",
  "main": "./lib/main",
  "version": "2.2.0",
  "description": "Elixir source code formatter for the Atom editor.",
  "keywords": [
    "elixir",
    "formatter"
  ],
  "activationCommands": {
    "atom-workspace": "atom-elixir-formatter:format"
  },
  "activationHooks": [
    "language-elixir:grammar-used"
  ],
  "repository": "https://github.com/rgreenjr/atom-elixir-formatter",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {}
}
